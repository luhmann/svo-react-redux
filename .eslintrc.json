{
  "extends": ["standard", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true,
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "mocha": true,
    "es6": true,
    "commonjs": true,
    "jest": true
  },
  "rules": {
    "quote-props": ["error", "as-needed"]
  },
  "plugins": [
    "react"
  ]
}
